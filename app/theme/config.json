{
  "colors": {
    "darkBlue": "#232F34",
    "darkBlue2": "#344955",
    "darkBlue3": "#4A6572",
    "ambar": "#F9AA33",
    "darkGray": "#424242",
    "darkGray1": "#616161",
    "darkGray2": "#757575",
    "gray": "#A5A3A3",
    "gray1": "#BFBFBF",
    "yellow": "#FFE800",
    "yellow1": "#FFE000",
    "darkYellow": "#C5B61D",
    "brightGreen": "#87F429",
    "green": "#2EB50F",
    "darkGreen": "#28950F",
    "darkGray3": "#353A3A",
    "gray2": "#60666B",
    "lightGray": "#F0F0F0",
    "lightGray1": "#F7F7F7",
    "lightGray2": "#FAFBFC",
    "red": "#FF0023",
    "lightBlue": "#A0D9F2",
    "blue": "#3F51B5",
    "skin": "F0DA82",
    "skin1": "#E4C984"
  },
  "typography": {
    "font": "Roboto",
    "variants": {
      "title": {
        "fontSize": "20px",
        "fontWeight": "bold",
        "color": "darkGray"
      },
      "subtitle": {
        "fontSize": "18px",
        "fontWeight": "bold",
        "color": "darkGray1"
      },
      "groupLabel": {
        "fontSize": "18px",
        "fontWeight": "bold",
        "color": "darkGray2"
      },
      "tableHeader": {
        "fontSize": "12px",
        "fontWeight": "bold",
        "color": "orange"
      },
      "tableBody": {
        "fontSize": "12px",
        "fontWeight": 400,
        "color": "gray1"
      },
      "tags": {
        "fontSize": "14px",
        "fontWeight": 400,
        "color": "darkGray"
      },
      "buttons": {
        "fontSize": "14px",
        "fontWeight": 400,
        "color": "darkGray"
      }
    }
  },
  "selectFields": {
    "checkbox": {
      "outlined": "darkGreen",
      "color": "darkGray",
      "$checked": {
        "filled": "darkGreen"
      },
      "$disabled": {
        "outlined": "lightGray1",
        "color": "lightGray2"
      }
    },
    "switch": {
      "outlined": "gray",
      "color": "darkGray",
      "$checked": {
        "filled": "darkGreen"
      }
    },
    "radio": {
      "outlined": "darkGreen",
      "$checked": {
        "filled": "darkGreen"
      },
      "$disabled": {
        "outlined": "lightGray1",
        "color": "lightGray2"
      }
    },
    "slider": {
      "outlined": "gray",
      "$selected": {
        "filled": "darkGreen"
      }
    }
  }
}
